<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE errors>

<errors>

  <defaults>
    <default type="Grammar error">
      <ids>
        <re v="real-.*"/>
        <e id="unreal-girjji"/>
      </ids>
      <header>
        <title xml:lang="en">Real-word error</title>
        <title xml:lang="se">Čállinmeattáhus</title>
      </header>
      <body>
        <description xml:lang="en">The word "$1" exists in the dictionary, but means something different in this context.</description>
        <description xml:lang="se">"$1" maŋis galggašii leat blah</description>
      </body>
    </default>

    <default type="Grammar error">
      <ids>
        <e id="default"/>
      </ids>
      <header>
        <title xml:lang="en">Grammar error</title>
        <title xml:lang="se">Čállinmeattáhus</title>
      </header>
      <body>
        <description xml:lang="en">The word "$1" exists in the dictionary, but means something different in this context.</description>
        <description xml:lang="se">"$1" maŋis galggašii leat blah</description>
      </body>
    </default>
  </defaults>

  <error id="msyn-arg-trm-ins">
    <header>
      <title xml:lang="en">Terminalis instead of Instrumentalis</title>
      <title xml:lang="kl">Terminalis og ikke instrumentalis</title>
    </header>
    <body>
      <description xml:lang="en">For soqu use instrumentalis</description>
      <description xml:lang="kl">For soqu brug instrumentalis</description>
    </body>
  </error>

  <error id="msyn-arg-ins-trm">
    <header>
      <title xml:lang="en">Instrumentalis instead of Terminalis</title>
      <title xml:lang="kl">Instrumentalis og ikke Terminalis</title>
    </header>
    <body>
      <description xml:lang="en">For Reci use Terminalis</description>
      <description xml:lang="se">For Reci brug terminalis</description>
    </body>
  </error>

  <error id="msyn-arg-abs-rel">
    <header>
      <title xml:lang="en">Relative instead of Absolutive</title>
      <title xml:lang="kl">Relativ og ikke Absolutiv</title>
    </header>
    <body>
      <description xml:lang="en">Use Relative instead of Absolutive for subject of transitive verb</description>
      <description xml:lang="kl">Hvis et subjekt, her "$1", står til et transitivt verb, her "$2", skal det stå i relativ og ikke i absolutiv.</description>
    </body>
  </error>


</errors>
