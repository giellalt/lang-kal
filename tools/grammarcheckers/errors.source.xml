<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE errors>

<errors>

  <defaults>
    <default type="Grammar error">
      <ids>
        <re v="real-.*"/>
        <e id="unreal-girjji"/>
      </ids>
      <header>
        <title xml:lang="en">Real-word error</title>
        <title xml:lang="da">Ekteordsfeil</title>
        <title xml:lang="kl">Čállinmeattáhus</title>
      </header>
      <body>
        <description xml:lang="en">The word "$1" exists in the dictionary, but means something different in this context.</description>
        <description xml:lang="da">"$1" maŋis galggašii leat blah</description>
        <description xml:lang="kl">"$1" maŋis galggašii leat blah</description>
      </body>
    </default>

    <default type="Grammar error">
      <ids>
        <e id="default"/>
      </ids>
      <header>
        <title xml:lang="en">Grammar error</title>
        <title xml:lang="da">Čállinmeattáhus</title>
        <title xml:lang="kl">Čállinmeattáhus</title>
      </header>
      <body>
        <description xml:lang="en">The word "$1" exists in the dictionary, but means something different in this context.</description>
        <description xml:lang="da">"$1" maŋis galggašii leat blah</description>
        <description xml:lang="kl">"$1" maŋis galggašii leat blah</description>
      </body>
    </default>
  </defaults>

  <error id="msyn-arg-trm-ins">
    <header>
      <title xml:lang="en">Terminalis instead of Instrumentalis</title>
      <title xml:lang="kl">Terminalis og ikke instrumentalis</title>
    </header>
    <body>
      <description xml:lang="en">For soqu use instrumentalis</description>
      <description xml:lang="kl">For soqu brug instrumentalis</description>
    </body>
  </error>

  <error id="msyn-arg-ins-trm">
    <header>
      <title xml:lang="en">Instrumentalis instead of Terminalis</title>
      <title xml:lang="kl">Instrumentalis og ikke Terminalis</title>
    </header>
    <body>
      <description xml:lang="en">For Reci use Terminalis</description>
      <description xml:lang="da">For Reci brug terminalis</description>
      <description xml:lang="kl">For Reci brug terminalis</description>
    </body>
  </error>

  <error id="msyn-arg-abs-rel">
    <header>
      <title xml:lang="en">Relative instead of Absolutive</title>
      <title xml:lang="da">Relativ og ikke Absolutiv</title>
      <title xml:lang="kl">Allamoorut taasiinnarniutaanngitsoq</title>
    </header>
    <body>
      <description xml:lang="en">Use Relative instead of Absolutive for subject of transitive verb</description>
      <description xml:lang="da">Hvis et subjekt, her "$1", står til et transitivt verb, her "$2", skal det stå i relativ og ikke i absolutiv.</description>
      <description xml:lang="kl">Susoq, uani "$1", oqaluummut susalimmut atagaangat, uunga "$2", allamoorutaasassaaq taasiinnarniutaanani.</description>
    </body>
  </error>

  <error id="msyn-abs-taama-case">
    <header>
      <title xml:lang="en">There should be case agreement in noun phrases</title>
      <title xml:lang="da"></title>
      <title xml:lang="kl"></title>
    </header>
    <body>
      <description xml:lang="en">The ending of "$1" needs to be the same as the word after</description>
      <description xml:lang="da"></description>
      <description xml:lang="kl"></description>
    </body>
  </error>

  <error id="typo">
    <header>
      <title xml:lang="en">"$1" seems misspelled</title>
      <title xml:lang="kl">"$1" naqinnerliugaarpasippoq</title>
    </header>
    <body>
      <description xml:lang="en">The word "$1" seems to be misspelled.</description>
      <description xml:lang="kl">Oqaaseq "$1" naqinnerliugaarpasippoq.</description>
    </body>
  </error>

  <error id="guillemets">
    <header>
      <title xml:lang="en">Wrong type of guillemets</title>
      <title xml:lang="kl">Isaasat kukkusut</title>
    </header>
    <body>
      <description xml:lang="en">You have used wrong kind of guillemets</description>
      <description xml:lang="kl">Isaasat kukkusut atorsimavatit.</description>
    </body>
  </error>

 <error id="ellipsis">
    <header>
      <title xml:lang="en">Ellipsis</title>
      <title xml:lang="kl">Minitsineq</title>
    </header>
    <body>
      <description xml:lang="en">You have used three periods, "$1". Here an ellipsis would have been a better choice.</description>
      <description xml:lang="kl">Naggat pingasoriarlugu atorsimaats, "$1". Minitsissutit atorukkit pitsaanerussaaq, "…".</description>
    </body>
  </error>
 
</errors>
