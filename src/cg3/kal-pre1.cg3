REMCOHORT:HeurOf ("of" ?) (-1 ("<University=of=.*>"r) + Heur/Prop OR ("<University=of=.*>"r) + Heur/N) ; #Jude 20240129
REMCOHORT:HeurProp (<university-of->) (-1 ("<University=of=.*>"r Heur/Prop)) ; #Jude 20240129
REMCOHORT:HeurProp (<university-of-X->) (-1 ("<University=of=.*>"r Heur/Prop)) ; #Jude 20240129

