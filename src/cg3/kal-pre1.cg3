# -*- cg-pre-pipe: zsh -l -c '~/langtech/kal/tools/shellscripts/kal-analyse --fst' -*-

# Nutserut/Gloss kal2eng pre-pipe: zsh -l -c '~/langtech/gloss/kal2eng/kal2eng.pl --fst'

INCLUDE STATIC disambiguator.cg3 ;

# kal-pre1.cg3 er nødvendig, fordi REMCOHORT fjerner en delimiter, hvilket betyder, at der efterlades et tomt vindue. Bundlinien er, at man ikke kan fjerne delimiters og derefter bruge den i samme CG



# ============ #
# Regelsektion #
# ============ #

BEFORE-SECTIONS


REMCOHORT:paragraftegn ("¶") + CLB IF (-*1W (":") + EOS OR (".") + EOS BARRIER EOS);
#BARRIER EOS for at sikre at det kun er den foregående sætning og ikke noget langt til venstre

MergeCohorts ("<$1,->"v "$1,-"v Num Abs Sg) ("<(.+)>"r Num Abs Sg) WITH (1 ("<,>")) (2 ("<->")) ;


SPLITCOHORT ("<$1>"v "$1"v Num Abs Sg "<$2>"v "$2"v CLB) ("<(\\p{N}+)([.])>"r Gram/Ord) (-1 BOS) (1 ("<\\p{Lu}.*"r)) (NOT 1 MAANED) ;

#Ad hoc disambiguering, fordi kalaallisooq=SSAQ + Via + Sg + 3SgPoss blokerer for hybridsplittet
REMOVE:kalaallisooq (/"kalaallisooq"\ SSAQ\ Der/nn\ N\ Via\ Sg\ 3SgPoss/l) IF (0 (/"kalaaleq"\ iN\ iAeq\ iPl\ Gram/Hyb\ UR\ Gram/IV\ SSA/l));

SPLITCOHORT ("<$1>"v "$1"v Num Abs Sg "<$2>"v "$2"v CLB) ("<(\\p{N}{4})([.])>"r Gram/Ord) (0 EOS) ;
SPLITCOHORT ("<$1>"v "$1"v Num Abs Sg "<$2>"v "$2"v CLB) ("<(\\p{N}{4})([.])>"r Gram/Ord) (1 ("¶") + EOS) ;

#splitting compounded by error
SPLITCOHORT ("<$1>"v "$1"v "<$2>"v "$2"v) TARGET ("<(\\p{Lu}?\\p{Ll}+)\\u003A(\\p{Lu}\\p{Ll}+)>"r ?) ; #Jude 20240202
SPLITCOHORT ("<$1>"v "$1"v "<$2>"v "$2"v) TARGET ("<(\\p{Lu}?\\p{Ll}+)(\\p{Lu}\\p{Ll}+)>"r ?) + HeurGLfinal ; #Jude 20240202
SPLITCOHORT ("<$1>"v "$1"v "<$2>"v "$2"v) TARGET ("<(\\d\\u002D\\p{Ll}+)(\\p{Lu}\\p{Ll}+)>"r ?) + HeurGLfinal ; #Jude 20240202
SPLITCOHORT ("<$1>"v "$1"v "<$2>"v "$2"v) TARGET ("<(\\p{Lu}?\\p{Ll}+)(\\p{Lu}+)>"r ?) + HeurGLfinal ; #Jude 20240202

#Heuristics
#Heur/Prop Heur/Quote
Substitute (".*"r) ("$1"v <quote-1>) TARGET ("<\(\\p{Lu}.*\)>"r) (-1 ("<(»|„)>"r) LINK 1* ("<(«|“)>"r) BARRIER CLB LINK 1* ("<(-|mi|ni|imi|ini|imiit|miit|mit|imut|mut|gooq|rooq)>"r) OR CLB) 
(NEGATE 0 ("<.*>"r) + WFORM LINK 0 ("$1"v) LINK NEGATE 1* ("<(-|mi|ni|imi|ini|imiit|miit|mit|imut|mut|gooq|rooq)>"r)) 
(NEGATE 0 TUQ) ;
Substitute (".*"r) ("$1"v <quote-2>) TARGET ("<\(.*\)>"r) (-1 (<quote-1>)) ; 
Substitute (".*"r) ("$1"v <quote-3>) TARGET ("<(«|“|i?mi|ni|i?mut|\\p{Ll}+)>"r) (-1 (<quote-2>)) ; 
Substitute (".*"r) ("$1"v <quote-4>) TARGET ("<(«|“|i?mi|ni|i?mut)>"r) (-1 (<quote-3>)) ;
Substitute (".*"r) ("$1"v <quote-5>) TARGET ("<(-|«|“|i?mi|ni|i?mut|i?mi?it)>"r) (-1 (<quote-4>)) ;  
Substitute (".*"r) ("$1"v <quote-6>) TARGET ("<(-|«|“|i?mi|ni|i?mut|i?mi?it)>"r) (-1 (<quote-5>)) ;  
Substitute:quoteAbsSg (".*"r PUNCT LEFT) ("<„$1$2$3>"v "$1"v Prop Abs Sg GUUQ Heur/Prop Heur/Quote) TARGET ("<„>") (1 ("<\(\\p{Lu}.*\)>"r) LINK 1 ("<\(“\)>"r) LINK 1 ("<\([g|r]ooq\)>"r)) ; # „Pharao“gooq 
Substitute:quoteAbsSg (".*"r PUNCT RIGHT) ("<»$1 $2$3>"v "$1 $2"v Prop Abs Sg Heur/Prop Heur/Quote) TARGET ("<»>") (1 ("<\(\\p{Lu}.*\)>"r <quote-1>) LINK 1 ("<\(.*\)>"r) LINK 1 ("<\(«\)>"r) LINK 1* 3SGSUBJVERB LINK NEGATE -1* _TARGET_ LINK -1* Abs + Sem/Hum BARRIER Abs - OTHERTIME - SEASON - Sem/dur) ; # »Oqaluttualiaativut taalliaativullu«
Substitute:quoteTrmSg (".*"r PUNCT RIGHT) ("<»$1 $2 $3$4$5>"v "$1 $2 $3"v Prop Abl Sg Heur/Prop Heur/Quote) TARGET ("<»>") (1 ("<\(\\p{Lu}.*\)>"r) LINK 1 ("<\(.*\)>"r) LINK 1 ("<\(.*\)>"r) LINK 1 ("<\(«\)>"r) LINK 1 ("<->") LINK 1 ("<\(i?mi?it\)>"r)) ; # »Fänrik Ståls sägner«-imiit
Substitute:quoteLokSg (".*"r PUNCT RIGHT) ("<»$1 $2$3$4>"v "$1 $2"v Prop Lok Sg Heur/Prop Heur/Quote) TARGET ("<»>") (1 ("<\(\\p{Lu}.*\)>"r) LINK 1 ("<\(.*\)>"r) LINK 1 ("<\(«\)>"r) LINK 1 ("<\(i?mi\)>"r)) ; # »Atuarfiit ilagiillu«mi
Substitute:quoteLokPl_Sg (".*"r PUNCT RIGHT) ("<»$1 $2$3$4>"v "$1 $2"v Prop Lok Pl Heur/Prop Heur/Quote) TARGET ("<»>") (1 ("<\(\\p{Lu}.*\)>"r) LINK 1 ("<\(.*\)>"r) LINK 1 ("<\(«\)>"r) LINK 1 ("<\(ni\)>"r)) ;  
COPY (Prop Lok Sg Heur/Prop Heur/Quote) EXCEPT (Prop Lok Pl Heur/Prop Heur/Quote) TARGET Lok + Pl + Heur/Prop ; # »Ukiut 300-nngornerat«ni
Substitute:quoteLokSg (".*"r PUNCT RIGHT) ("<»$1$2$3>"v "$1"v Prop Lok Sg Heur/Prop Heur/Quote) TARGET ("<»>") (1 ("<\(\\p{Lu}.*\)>"r) LINK 1 ("<\(«\)>"r) LINK 1 ("<\(i?mi\)>"r)) ; # »Atuarfiit«mi
Substitute:quoteTrmSg (".*"r PUNCT RIGHT) ("<»$1$2$3>"v "$1"v Prop Trm Sg Heur/Prop Heur/Quote) TARGET ("<»>") (1 ("<\(\\p{Lu}.*\)>"r <quote-1>) LINK 1 ("<\(«\)>"r) LINK 1 ("<\(i?mut\)>"r)) ; # »Kamikken«imut
Substitute:quoteTrmSg (".*"r PUNCT RIGHT) ("<»$1 $2$3$4>"v "$1 $2"v Prop Trm Sg Heur/Prop Heur/Quote) TARGET ("<»>") (1 ("<\(\\p{Lu}.*\)>"r) LINK 1 ("<\(.*\)>"r) LINK 1 ("<\(«\)>"r) LINK 1 ("<\(i?mut\)>"r)) ; # »Kamikken«imut
Remcohort (<quote-1>) ;
Remcohort (<quote-2>) ;
Remcohort (<quote-3>) ;
Remcohort (<quote-4>) ;
Remcohort (<quote-5>) ;
Remcohort (<quote-6>) ;

#Heur abbr
Substitute ("<.*>" ?) ("$1"v N Gram/Abbr Abs Sg Heur/Abbr) TARGET ("\(\\p{Ll}\\p{Ll}\)"r ?) - For2Let ; #Jude 20240208
Substitute ("<.*>" ?) ("$1"v N Gram/Abbr Abs Sg Heur/Abbr) TARGET ("\(\\p{Lu}\\p{Ll}\)"r ?) - For2Let - HeurExcl ; #Jude 20240208
Substitute ("<.*>" ?) ("$1"v N Gram/Abbr Abs Sg Heur/Abbr) TARGET ("\(\\p{Ll}\\p{Lu}\)"r ?) - For2Let - HeurExcl ; #Jude 20240208
#Heur/Excl
Substitute:HeurExcl ("<.*>"r ?) ("$1"v Heur/Excl) TARGET ("<\(.*\)>"r ?) + HeurExcl - HeurGLfinal - HeurGLErr - HeurConsIllGL - RomNum ; #Jude 20240206

# Specific heuristic analysis first
#Heur University of
SUBSTITUTE (?) (<university-of-> ?) TARGET ("\\p{Lu}.*"r ?) (-1 ("of") LINK -1 ("University")) ; #Jude 20240129
SUBSTITUTE (Sem/Geo Prop Abs Sg) (Sem/Geo Prop Abs Sg <university-of->) TARGET ("\\p{Lu}.*"r) (-1 ("of") LINK -1 ("University")) ; #Jude 20240129
SUBSTITUTE (Sem/Geo Prop Abs Sg) (Sem/Geo Prop Abs Sg <university-of-X->) TARGET ("\\p{Lu}.*"r) (-1 (<university-of->) LINK -1 ("of") LINK -1 ("University")) 
(NEGATE 0 ("<(\\p{Lu}[\\p{Ll}\\p{N}\\p{M}]+?)-?i?[mi|mut]>"r)) ; #Jude 20240129

MergeCohorts:HeurProp_Rel_1 ("<$1=$2=$3$4>"v "$1=$2=$3"v Prop Rel Sg Heur/Prop) ("<\(University\)>"r ?) WITH (1 ("<\(of\)>"r ?) LINK 1 ("<(\\p{Lu}[\\p{Ll}\\p{N}\\p{M}]+?)\(-?i?p\)>"r)) ; #Jude 20240129
MergeCohorts:HeurProp_Rel2 ("<$1=$2=$3=$4$5>"v "$1=$2=$3=$4"v Prop Rel Sg Heur/Prop) ("<\(University\)>"r ?) WITH (1 ("<\(of\)>"r ?) LINK 1 ("<(\\p{Lu}[\\p{Ll}\\p{N}\\p{M}]+?)>"r ?) LINK 1 ("<(\\p{Lu}[\\p{Ll}\\p{N}\\p{M}]+?)\(-?i?p\)>"r)) ; #Jude 20240129

