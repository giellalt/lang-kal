SET TRANSVERB = 1SgO | 2SgO | 3SgO | 4SgO | 1PlO | 2PlO | 3PlO | 4PlO ;
SET INTRANSVERB = V - TRANSVERB ;
SET VFIN = Ind | Opt | Int | Imp ;
