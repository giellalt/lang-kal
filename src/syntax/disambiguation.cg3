SET VERB-NOTINF = V - Inf - InfNeg ;
SET INF = Inf OR InfNeg ;
SET CON_CAU = Con OR Cau ;
SET PASSIV = (GE SIMA) OR NIQAR OR (TRANSVERB TAQ U) ;
LIST NO_HTR = "allap" ; #20091108 - Verber, der ikke kan kombinere med HTR
SET SchwaDerivativ = GE | TARE | QE ;

