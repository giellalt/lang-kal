BEFORE-SECTIONS

DELIMIT ("<">") IF (-1W _S_DELIMITERS_) ; #virker tilsyneladende ikke

#####################################
# Cleaning special constructions that complicate the rest #
#####################################

# Appositional proper noun without comma
REMOVE:0001 Prop + Attr IF (0 Prop + Sg)(-1 N + Sg)(*1 VFIN + 3Sg BARRIER KOMMA);
# Same construct case LU
REMOVE:0002 Prop + Attr IF (0 Prop + Sg)(-1 N + Sg)(1 N + Sg + LU)(*1 VFIN + 3Pl BARRIER KOMMA) ;

#Vælg Indikativ verbum, hvis eneste finitte. NIR undtaget, da ofte fejl for NIQ plus noget
SELECT:0003 Ind IF (NEGATE *0 VFIN BARRIER LU OR LI OR KOMMA)(NEGATE 0 NIR)(NEGATE 0 Int) ;

#Fjern umulige possessum
REMOVE:0004 POSSESSUM IF (0 NON-POSSESSUM)(NEGATE 0 TAQ + nn) ;

#KKUT vs. Via
REMOVE:0005 KKUT IF (0 GeoProp);
REMOVE:0006 ("qaap") IF (0 ("qaammat"));
# Vælg Interj hvis fulgt af '?' eller efter foregående Imp
SELECT:0010 ("aat") IF (1 ("?"));
SELECT:0011 ("aat") IF (-*1C Imp);

REMOVE:0012 ("ipi") + RAR IF (0 ("iperar"));
REMOVE:0013 ("iper") + RAR IF (0 ("iperar"));
REMOVE:0014 ("ipip") + RAR IF (0 ("iperar"));
REMOVE:0015 ("pissap") + TRANSVERB IF (NEGATE *0 ANIMAL);
REMOVE:0016 TIME + 4SgPoss IF (0 Lok); #Vælger N+Lok frem for N+4SgPoss

#siornatigut sjældent possessum, oftest i-ADVL> til adled
SELECT:0017 ("siornatigut") + part IF (1 vn) ;

#'din luvart' fremfor 'meget' næsten utænkelig
REMOVE:0018 ("assoq") + 2SgPoss IF (0 ("assut") + part);

#Vokativ meget sjælden uden proprium
REMOVE:0019 AA IF (NEGATE 0 Prop);

#"Måtte han su" er ret usandsynlig
REMOVE:0020 ("su") + Opt IF (0 ("suli") + part ) ;

#'last' som isoleret ord nærmest utænkelig
REMOVE:0021 ("usi") + Abs IF (0 ("usi") + Interj)(-1 BOS OR KOMMA)(1 KOMMA);

#Usandsynlige morfemkombinationer
REMOVE:0022 TUQ + LIR + nv IF (0 ("ilungersor") + LIR) ;

SELECT:0023 ("illuttut") + part IF (*1 LIK OR QAR BARRIER V); #Næppe sneblinde, men tosidet N

#TAQ vs. GE skaber problemer. Vælg altid GE, hvis mulig sammenblanding med TAQ og hvis ikke andre finitte verber i sætningen
SELECT:0024 GE + TRANSVERB IF (0 TAQ)(NEGATE *0 VFIN);

#Farlig regel, der skal checkes, men så længe både (3Pl+3PlO+SUB) og (3Sg+3PlO) eksisterer er det umuligt at disambiguere Rel+Pl som subjekt fra Abs+Pl som objekt.
REMOVE:0026 3Pl + 3PlO + SUB IF (0 3Sg + 3PlO)(*-1 Abs + Pl ) ;

#lokalis eller 4SgPoss: Næppe possessum hvis taamaattuni er adled
SELECT:0027 Lok + Pl IF (1 ("taamak") + Lok + Pl) ;
SELECT:0028 ("taamak") + Lok + Pl IF (-1 Lok + Pl) ;

#Transitiv participium som absolut tid
SELECT:0029 U + Par + 3Sg + 3SgO ;

#Absolut tidsangivelse i Abs med possessor
SELECT:0030 TIME + Rel IF (-1 BOS)(1 ("ila") + 3SgPoss + Abs LINK *1 T:NexusHøjre ) ;
SELECT:0031 ("ila") + 3SgPoss + Abs IF (-1 TIME + Rel)(*1 T:NexusHøjre ) ;

#Rel + 4SgPoss sammenblandes ofte med Lok. Derfor en tidlig regel til at selecte sikre subjekter
SELECT:0036 Rel + 4SgPoss IF (*1 TRANSVERB + 3Sg + Con BARRIER V)(NEGATE 0 Interj)(NEGATE 0 V)(NEGATE 1 3SgPoss)(NEGATE 0 VIK + Lok)(NEGATE 0 Rel + Pl LINK 1 POSSESSUM + 3PlPoss); #fx Ikke Ilami! og anillakkami, ikke possessor og ikke ADVL
SELECT:0037 Rel + 4SgPoss IF (*1 TRANSVERB + 3Sg + Cau BARRIER V)(NEGATE 0 Interj)(NEGATE 0 V)(NEGATE 1 3SgPoss)(NEGATE 0 VIK + Lok)(NEGATE 0 Rel + Pl LINK 1 POSSESSUM + 3PlPoss); #fx Ilami! og anillakkami
SELECT:0038 Rel + 4SgPoss IF (*1 TRANSVERB + 3Sg + Par BARRIER V)(NEGATE 0 Interj)(NEGATE 0 V)(NEGATE 1 3SgPoss)(NEGATE 0 VIK + Lok)(NEGATE 0 Rel + Pl LINK 1 POSSESSUM + 3PlPoss); #fx Ilami! og anillakkami
REMOVE:0039 Abs + 3SgPoss IF (0 Int + 3Sg)(1 ("?")); # .. toqua? er ikke 'hendes død'
REMOVE:0039A ("sani") + LIK IF ( 0 ("sanileq"));
SELECT:0039B part + ("aamma") ;

REMOVE:0040A TIME + KKUT IF (-1 BOS); #Kan vist kun være Via i den position



###################################
# Frequent and safe rules - nouns #
###################################

REMOVE:0040 ("ilaaq") - AFFINGERET IF (0 ("ila") + POSSESSUM) ; #'ilaaq' uden QAR el. lign. usandsynlig
REMOVE:0041 ("isu") + MAA IF (0 ("isuma")); 

SELECT:0042 Comp IF (1 ("ajor") OR ("artor") OR ("saper") OR ("nalu"));
REMOVE:0043 Comp IF (NEGATE 1 ("ajor") OR ("artor") OR ("saper") OR ("nalu"));
