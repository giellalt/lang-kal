REMOVE:0045 ("oqaq") + UTE IF ( 0 ("oqar") );
#fx oqar+UTE+GE frem for oqaq+UTE+GE
 
SELECT:0046 Num + 3PlPoss IF (-1 MAANED);
#NB Possessum skal vaere plur. da konsekvent underforstaaet 'ulloq' MAANED (ulluisa) Num

SELECT:0050 ("inngili") IF (-1 ("nuiaq") + Rel OR ("naalap") + TAQ + Rel OR ("naalagaq") + Rel) ; #Skal måske udvides til et set??
SELECT:0051 ("kalaaleq") + Rel + Pl IF (1 ("oqaaseq") OR ("nuna") OR ("Nuna") OR ("oqar") + USIQ);
SELECT:0052 ("oqaaseq") + 3PlPoss IF (-1 ("kalaaleq") + Rel + Pl);
SELECT:0053 ("oqar") + USIQ + 3PlPoss IF (-1 ("kalaaleq") + Rel + Pl);
SELECT:0054 ("Nuna") + Sg + 3PlPoss IF (-1 ("kalaaleq") + Rel + Pl);
SELECT:0055 ("nuna") + Sg + 3PlPoss IF (-1 ("kalaaleq") + Rel + Pl);
#Kalaallit Nunaat, kalaallit oqaasii etc.

REMOVE:0059 ("nuna") + 1PlPoss + Pl IF (0 ("nuna") + 1PlPoss + Sg); #Nunatsinni altid Sg
REMOVE:0060 Abs + Sg + MI IF (*0 Abs + Sg) ; #20101013 Lokalis tagget som subjekt med MI er en stor fejlkilde

SELECT:0061 Rel + Pl IF (1 DemPron + Rel LINK 1 3PlPoss);
SELECT:0062 Rel + Sg IF (1 DemPron + Rel LINK 1 3SgPoss);
REMOVE:0063X 3SgPoss IF (0 3PlPoss LINK *-1 Rel + Pl BARRIER KOMMA )(NEGATE *-1 Rel + Sg)(NEGATE *-1 3PlPoss LINK *-1 Rel + Pl); #nødvendig disambiguering før 0063. Sidste NEGATE på gr. af helt andre possessum længere til venstre

SELECT:0063 Rel + Pl IF (1C 3PlPoss)(NEGATE 1 ("siuneq") + Via)(NEGATE 0 NON_HUMAN LINK 1 humverb) ; #må ikke hitte på siornatigut
SELECT:0064 Rel + Sg IF (1C 3SgPoss)(NEGATE 0 Ins LINK 1 QAR OR LIK)(NEGATE 1 LU)(NEGATE 0 ("affaq"))(NEGATE 0 PLACE + 4SgPoss ); #asanninnermik pigisaqarneranut er ikke en possessor-possessum forbindelse
SELECT:0065 3SgPoss IF (-1C Rel + Sg)(NEGATE *1 VERB-NOTINF + TRANSVERB + 3Sg BARRIER VFIN);

#Nexusadverbialer med kinguneq og nali
SELECT:0067 ("kinguneq") + 3SgPoss IF (-2 BOS OR KOMMA)(-1 Rel)(NEGATE 0 ("naamik")) ;
SELECT:0068 ("n") + 3SgPoss IF (-2 BOS OR KOMMA)(-1 Rel)(NEGATE 0 ("naamik")) ;

#Sikre possessorer. Den foerste er et specialtilfaelde for også at tage højde for 'Maaniittut ukua ilaasa .. '

