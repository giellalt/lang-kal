MAP:M0225C @ADVL> TARGET ("aamma") + Adv IF (*1 TIME BARRIER (*) - ("tassa") )(NEGATE -1 BOS OR KOMMA) ; 
SELECT:0225D Adv IF (0 ("aamma") + TTAAQ) ;


 
