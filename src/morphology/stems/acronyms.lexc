%„arnanut% højskoli%“   acroconnector ;
%„demokrati% sunaana%“ acroconnector ;

LEXICON smallacro
as ACRO ; !A/S
cd ACRO ;
tv ACRO ;

LEXICON ACRO               
%-      arabiskeefterkomma ; ! Here we dive over to the numerals, 
        arabiskeefterkomma ; ! Here we dive over to the numerals
+N+ACR: acroconnector      ; ! Here we add the POS tag and go ACRO

LEXICON ACRO_C
%-      arabiskeefterkomma ; ! Here we dive over to the numerals, 
        arabiskeefterkomma ; ! Here we dive over to the numerals
+N+ACR: acroconnector_C    ; ! Here we add the POS tag and go ACRO

LEXICON acroconnector  
    :%- Naalsuf ;             
+Abs:   #       ;

LEXICON acroconnector_C  
    :%-i Naalsuf ;             
+Abs:    #       ;

